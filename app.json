{
  "expo": {
    "name": "DeskApp",
    "slug": "deskapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "deskapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.deskapp.deskapp",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to capture and import documents to your desk.",
        "NSMicrophoneUsageDescription": "This app may use the microphone for voice notes and document scanning features.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to import images and documents to your desk."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f962f719-4974-4903-9304-48ae2cdc6842"
      }
    }
  }
}